#include <iostream>

using namespace std;

int main()
{
    int y=0,nr,divizori,i,x;
    cin>>x;
    nr=2;
    while(y<x)
    {
        divizori=0;
        for(i=1;i<=nr;i++)
            if(nr%i==0)
                divizori++;
        if(divizori==2)
            y++;
        nr++;
    }
    cout<<nr-1;
}
