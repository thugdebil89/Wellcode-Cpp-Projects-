#include <iostream>

using namespace std;

int main()

{

   int N, M, k, v[1000001], lungime_sir;

   cin>>N;

   lungime_sir=N;

   for (int i=1; i<=lungime_sir; i++)

       cin>>v[i];

   cin>>M;

   for (int j=1; j<=M; j++)

   {

       cin>>k;

       if (k==1)

       {

           int X;
           cin>>X;

           lungime_sir++;

           v[lungime_sir]=X;

       }

       if (k==2)

           lungime_sir--;

   }

     cout<<lungime_sir<<endl;

   for (int i=1; i<=lungime_sir; i++)


      cout<<v[i]<<" ";
return 0;
    
}

